<?xml version="1.0" encoding="utf-8"?>
<keyboard name="QWERTY (custom)" script="latin">
  <row>
    <key key0="q" key1="!" key2="1" key4="esc"/>
    <key key0="w" key1="&quot;" key2="2"/>
    <key key0="e" key1="loc €" key2="3"/>
    <key key0="r" key1="$" key2="4"/>
    <key key0="t" key1="%" key2="5" key3="*"/>
    <key key0="y" key1="&amp;" key2="6"/>
    <key key0="u" key1="/" key2="7" key3="^"/>
    <key key0="i" key1="(" key2="8" key3="[" key4="{"/>
    <key key0="o" key1=")" key2="9" key3="]" key4="}"/>
    <key key0="p" key1="=" key2="0" key3="'" key4="\?"/>
  </row>
  <row>
    <key shift="0.5" key0="a" key1="tab" key2="`"/>
    <key key0="s" key1="loc accent_ring" key2="loc §" key3="loc ß" key4="loc accent_ogonek"/>
    <key key0="d" key1="loc accent_grave" key2="loc £" key3="loc accent_aigu"/>
    <key key0="f" key1="loc accent_dot_above"/>
    <key key0="g" key1="loc accent_caron" key2="-" key3="_"/>
    <key key0="h" key1="ℏ" key3="+"/>
    <key key0="j" key1="loc accent_trema" key2="loc accent_circonflexe"/>
    <key key0="k" key1="loc accent_double_aigu" key2="\#" key3="\@"/>
    <key key0="l" key1="f11_placeholder" key2="|" key3="\\" key4="f12_placeholder"/>
  </row>
  <row>
    <key width="1.5" key0="shift" key2="loc capslock"/>
    <key key0="z" key1="&lt;" key3="&gt;"/>
    <key key0="x" key2="loc †"/>
    <key key0="c" key1="loc accent_cedille" key3="."/>
    <key key0="v" key3=","/>
    <key key0="b" key1="~"/>
    <key key0="n" key1="loc accent_tilde" key2=":" key3=";"/>
    <key key0="m"/>
    <key width="1.5" key0="backspace" key2="delete"/>
  </row>
</keyboard>
